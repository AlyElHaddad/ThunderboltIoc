  </PropertyGroup>

  <ImportGroup>
    <Import Project="$(ParentDirectoryBuildPropsPath)" />
  </ImportGroup>

  <PropertyGroup>
    <PackageOutputPath>..\..\..\output_packages</PackageOutputPath>
