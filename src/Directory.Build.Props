<Project>
  <PropertyGroup>
    <Version>1.1.1</Version>
    <Authors>Aly Elhaddad</Authors>
    <Owners>Aly Elhaddad</Owners>
    <PackageProjectUrl>https://github.com/AlyElhaddad/ThunderboltIoc</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AlyElhaddad/ThunderboltIoc</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>Thunderbolt;ThunderboltIoc;roslyn;analyzers;code-generation;ioc;dependency-injection;di;inversion-of-control;aot</PackageTags>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>images\thunderbolt_128.png</PackageIcon>
    <PackageReadmeFile>docs\README.md</PackageReadmeFile>
    <PackageOutputPath>..\..\output_packages</PackageOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\assets\thunderbolt_128.png" Pack="true" Visible="false" PackagePath="images\thunderbolt_128.png"/>
    <None Include="..\..\README.md" Pack="true" Visible="false" PackagePath="docs\README.md"/>
  </ItemGroup>

</Project>